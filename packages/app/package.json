{
  "name": "marks",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "watchFile": "node watchFileChange.js",
    "test": "jest",
    "lint": "eslint .",
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "aab": "npx jetify && cd android && ./gradlew clean && ./gradlew bundleProduction && cd ../",
    "apkDev": "npx jetify && cd android && ./gradlew clean && ./gradlew assembleDev && cd ../",
    "apkStaging": "npx jetify && cd android && ./gradlew clean && ./gradlew assembleStaging && cd ../",
    "genimg": "node genimg src/assets/images --output src/res/images.ts --name images",
    "postinstall": "npx patch-package && npx typesync && RCT_NEW_ARCH_ENABLED=0  npx pod-install ios",
    "typesync": "npx typesync",
    "build:DevAndroid": "cd ./android && fastlane build --env dev",
    "build:DevIos": "cd ./ios && fastlane build --env dev",
    "build:StagingAndroid": "cd ./android && fastlane build --env staging",
    "build:StagingIos": "cd ./ios && fastlane build --env staging",
    "build:ProductionAndroid": "cd ./android && fastlane build --env production",
    "build:ProductionIos": "cd ./ios && fastlane build --env production",
    "genfont": "node genimg src/assets/fonts --output src/res/fonts.ts --name fonts",
    "codePushProduction": "sh release-app.sh Production",
    "codePushDev": " sh release-app.sh dev 1.0.6",
    "codePushStaging": " sh release-app.sh staging 1.0.7"
  },
