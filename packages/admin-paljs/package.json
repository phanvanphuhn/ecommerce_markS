  "scripts": {
    "dev": "next -p 8000",
    "build": "next build",
    "start": "next start -p 8000",
    "generate": "pal g && yarn generate:prisma && yarn generate:nexus && yarn codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "yarn ts-node --transpile-only src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev",
    "db-deploy": "prisma migrate deploy",
    "db-reset": "prisma migrate reset",
    "db-seed": "prisma db seed --preview-feature",
    "ts-node": "ts-node -r tsconfig-paths/register -P tsconfig-nexus.json"
  },
