{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest",
      "args": ["${workspaceFolder}/src/main.ts"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:dev", "--", "--inspect-brk"],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "protocol": "inspector",
      "sourceMaps": true
    }
  ]
}
